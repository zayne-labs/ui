{
	"name": "[*]",
	"type": "module",
	"version": "0.0.0",
	"packageManager": "pnpm@9.12.2",
	"description": "[*]",
	"author": "[*]",
	"license": "MIT",
	"homepage": "https://github.com/[*]#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/[*].git"
	},
	"bugs": {
		"url": "https://github.com/[*]/issues"
	},
	"keywords": [],
	"sideEffects": false,
	"exports": {
		".": "./dist/esm/index.js"
	},
	"main": "./dist/esm/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=18.x"
	},
	"scripts": {
		"build": "tsup",
		"build:dev": "cross-env NODE_ENV=development tsup",
		"build:test": "concurrently --prefix-colors \"yellow.bold,#7da4f8.bold,magenta\" --names PUBLINT,TSUP 'pnpm:lint:publint' 'pnpm:build:dev'",
		"lint:attw": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
		"lint:check-types": "tsc --pretty --incremental -p tsconfig.json",
		"lint:publint": "publint --strict .",
		"lint:size": "size-limit",
		"release": "changeset publish",
		"test:release": "pkg-pr-new publish"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "0.17.0",
		"@size-limit/esbuild-why": "11.1.6",
		"@size-limit/preset-small-lib": "11.1.6",
		"@total-typescript/ts-reset": "0.6.1",
		"@zayne-labs/tsconfig": "0.2.1",
		"concurrently": "^9.0.1",
		"cross-env": "^7.0.3",
		"publint": "^0.2.11",
		"size-limit": "11.1.6",
		"terser": "5.36.0",
		"tsup": "8.3.5",
		"typescript": "5.7.2"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"provenance": false
	},
	"size-limit": [
		{
			"path": "./src/index.ts",
			"limit": "[*] kb"
		},
		{
			"path": "./src/[*]/index.ts",
			"limit": "[*] kb"
		}
	]
}
