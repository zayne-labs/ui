{
	"name": "@zayne-labs/ui-react",
	"type": "module",
	"version": "0.10.51",
	"description": "A composable UI/UI-utilities components library. ",
	"author": "Ryan Zayne",
	"license": "MIT",
	"homepage": "https://zayne-labs-ui.vercel.app",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zayne-labs/ui.git"
	},
	"bugs": {
		"url": "https://github.com/zayne-labs/ui/issues"
	},
	"keywords": [],
	"sideEffects": false,
	"exports": {
		".": null,
		"./ui/*": "./dist/esm/ui/*/index.js",
		"./common/*": "./dist/esm/common/*/index.js",
		"./css/*": "./css/*",
		"./style.css": "./dist/style.css"
	},
	"files": [
		"css",
		"dist"
	],
	"engines": {
		"node": ">=18.x"
	},
	"scripts": {
		"build": "concurrently 'pnpm:build:tailwind' 'tsdown'",
		"build:dev": "cross-env NODE_ENV=development tsdown",
		"build:tailwind": "tailwindcss -i=css/style.css -o=./dist/style.css",
		"build:test": "concurrently --prefix-colors \"yellow.bold,#7da4f8.bold,magenta\" --names PUBLINT,TSUP 'pnpm:lint:publint' 'pnpm:build:dev'",
		"dev": "pnpm build:dev --watch",
		"lint:attw": "attw --pack . --profile esm-only --format table-flipped --ignore-rules=cjs-resolves-to-esm --ignore-rules=named-exports",
		"lint:eslint": "eslint . --max-warnings 0",
		"lint:eslint:interactive": "pnpx eslint-interactive@latest . --max-warnings 0 --fix",
		"lint:format": "prettier --write .",
		"lint:publint": "publint --strict .",
		"lint:size": "size-limit",
		"lint:type-check": "tsc --pretty -p tsconfig.json",
		"release": "pnpm publish --no-git-checks",
		"release:test": "pnpx pkg-pr-new publish"
	},
	"peerDependencies": {
		"react": "19.x.x",
		"react-dom": "19.x.x",
		"react-hook-form": "7.x.x",
		"tailwind-merge": "^2.5.0 || ^3.0.0",
		"tailwindcss": "4.x.x"
	},
	"peerDependenciesMeta": {
		"react-hook-form": {
			"optional": true
		},
		"tailwindcss": {
			"optional": true
		}
	},
	"dependencies": {
		"@zayne-labs/toolkit-core": "0.12.36",
		"@zayne-labs/toolkit-react": "0.12.36",
		"@zayne-labs/toolkit-type-helpers": "0.12.36"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "0.18.2",
		"@size-limit/esbuild-why": "12.0.0",
		"@size-limit/preset-small-lib": "12.0.0",
		"@tailwindcss/cli": "4.1.18",
		"@total-typescript/ts-reset": "0.6.1",
		"@types/react": "19.2.13",
		"@types/react-dom": "19.2.3",
		"@zayne-labs/prettier-config": "0.11.27",
		"@zayne-labs/tsconfig": "0.11.27",
		"concurrently": "9.2.1",
		"cross-env": "10.1.0",
		"prettier": "3.8.1",
		"prettier-plugin-classnames": "0.9.0",
		"prettier-plugin-merge": "0.9.0",
		"prettier-plugin-tailwindcss": "0.7.2",
		"publint": "0.3.17",
		"publish": "0.6.0",
		"react": "19.2.4",
		"react-dom": "19.2.4",
		"react-hook-form": "7.71.1",
		"size-limit": "12.0.0",
		"tailwind-merge": "3.4.0",
		"tailwindcss": "4.1.18",
		"tsdown": "0.20.3",
		"typescript": "5.9.3"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"provenance": true
	},
	"size-limit": [
		{
			"path": "src/components/ui/**/index.ts",
			"limit": "8 kB"
		},
		{
			"path": "src/components/common/**/index.ts",
			"limit": "1 kb"
		}
	]
}
